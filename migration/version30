create table edgenode.user_qualification (
 id bigserial primary key,
 provider_branch_qualification_fk int8 not null
  constraint "user_trajectory__provider_branch_qualification_id_fk"
  references edgenode.provider_branch_qualification(id),
 user_fk int8 not null constraint "user_qualification___user_id_fk" references auth.user(id),
 value text not null,
 constraint "user_qualification__provider_branch_qualification_fk__user_fk__uniq" unique (user_fk, provider_branch_qualification_fk));