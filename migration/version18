create table edgenode.provider_branch_qualification_dependency (
  id bigserial primary key,
  cluster int4 not null,
  required_degree text,
  provider_branch_qualification_fk int8 not null
  constraint "provider_branch_qualification_dependency__provider_branch_qualification_id_fk" 
  references edgenode.provider_branch_qualification(id), 
  created timestamptz not null,
  modified timestamptz,
  deleted timestamptz,
  is_deleted bool default false);