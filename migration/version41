create table edgenode.provider_branch_promoted_qualification_user (
  id bigserial primary key,
  user_fk int8 not null constraint "provider_branch_promoted_qualification_user___user_id_fk" references auth.user(id),
  provider_branch_qualification_fk int8 not null
  constraint "provider_branch_promoted_qualification_user__provider_branch_qualification_id_fk"
  references edgenode.provider_branch_qualification(id),
  trajectory_status text not null,
  created timestamp not null default now(),
  processed timestamp null,
  promoted_type text not null);

create table edgenode.provider_pool_tags (
  id bigserial primary key,
  title text not null,
  qualification_fk int8 not null
  constraint "provider_pool_tags__provider_branch_qualification_id_fk"
  references edgenode.provider_branch_qualification(id));

create table edgenode.provider_pool_tags_value (
  value text not null,
  tags_fk int8 not null
  constraint "provider_pool_tags_value__tags_id_fk"
  references edgenode.provider_pool_tags(id),
  primary key (value, tags_fk));